		// Helper function to safely get the value of a radio button group
		function getRadioValue(groupName) {
			const radioButtons = document.querySelectorAll(`input[name="${groupName}"]:checked`);
			return radioButtons.length > 0 ? radioButtons[0].value : null;
		}
	
		// Helper function to set the value of a radio button group
		function setRadioValue(groupName, value) {
			const radioButtons = document.querySelectorAll(`input[name="${groupName}"]`);
			radioButtons.forEach(radioButton => {
				radioButton.checked = radioButton.value === value;
			});
		}
