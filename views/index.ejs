	<script>
		// Function to navigate to different sections based on dropdown selection
		function navigateToSection() {
		  var selectedOption = document.getElementById("dropdown");
		  var chosenValue = selectedOption.options[selectedOption.selectedIndex].getAttribute('data-value');
	  
		  // Hide all sections initially
		  var sections = document.querySelectorAll(".multisteps-form__panel");
		  sections.forEach(section => {
			section.style.display = "none";
		  });
	  
		  // Display the selected section
		  var selectedSection = document.getElementById("section" + chosenValue);
		  if (selectedSection) {
			selectedSection.style.display = "block";
		  }
		}

		

	
	
